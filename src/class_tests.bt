< class Person(< name: str, < num: int)
{
    @ set_name(name: str)
    {
        .name = name;
    }

    < get_name(): str
    {
        return .name;
    }

    @ set_num(num: int)
    {
        .num = num;
    }

    < get_num(): int
    {
        return .num;
    }

