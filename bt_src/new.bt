# Demonstration of  `let` variable #

let a = 15;
let b = 5 + 6;
let c = 56.8 - 2.6;
let d = 45.0 + 56;
let e = a * b / c;

let abc = (c + a) * b / 1000;

printl(`a = [a], b = [b], c = [c], d = [d], e = [e], abc = [abc]`);


@ sum(num1: int, num2: int): float
{ 
    let r = 9 * 567.0;
    printl(`r = [r]`);
    return r + num1 + num2;
}

let z = sum(1, 2);




















